  "bioReactiveTemplates": [
    {
      "name": "Calm to Energetic",
      "description": "HRV controls filter cutoff for smooth calm/energetic transitions",
      "mapping": {
        "hrv": {
          "parameter": "filterCutoff",
          "range": [200, 8000],
          "curve": "linear"
        },
        "coherence": {
          "parameter": "resonance",
          "range": [0.1, 0.9],
          "curve": "exponential"
        },
        "stress": {
          "parameter": "detune",
          "range": [0.0, 0.3],
          "curve": "linear"
        }
      }
    },
    {
      "name": "Meditation Mode",
      "description": "Low stress = warm pads, high stress = bright tones",
      "mapping": {
        "hrv": {
          "parameter": "reverbSize",
          "range": [0.3, 0.95],
          "curve": "logarithmic"
        },
        "coherence": {
          "parameter": "grainSize",
          "range": [50, 150],
          "curve": "linear"
        },
        "stress": {
          "parameter": "brightness",
          "range": [0.2, 0.9],
          "curve": "inverse"
        }
      }
    },
    {
      "name": "Focus Enhancer",
      "description": "Coherence drives harmonic content for focus",
      "mapping": {
        "coherence": {
          "parameter": "harmonicContent",
          "range": [0.3, 1.0],
          "curve": "linear"
        },
        "hrv": {
          "parameter": "attackTime",
          "range": [5, 500],
          "curve": "exponential"
        },
        "stress": {
          "parameter": "noiseLevel",
          "range": [0.0, 0.2],
          "curve": "inverse"
        }
      }
    },
    {
      "name": "Sleep Inducer",
      "description": "Low HRV = deep bass, high coherence = gentle pads",
      "mapping": {
        "hrv": {
          "parameter": "frequency",
          "range": [40, 200],
          "curve": "inverse"
        },
        "coherence": {
          "parameter": "reverbTime",
          "range": [2.0, 8.0],
          "curve": "linear"
        },
        "stress": {
          "parameter": "filterCutoff",
          "range": [500, 2000],
          "curve": "inverse"
        }
      }
    }
  ],

