<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- ============================================================ -->
	<!-- HEALTHKIT -->
	<!-- ============================================================ -->

	<!-- HealthKit Access -->
	<key>com.apple.developer.healthkit</key>
	<true/>
	<key>com.apple.developer.healthkit.access</key>
	<array>
		<string>health-records</string>
	</array>

	<!-- Background Health Delivery -->
	<key>com.apple.developer.healthkit.background-delivery</key>
	<true/>

	<!-- Workout Processing -->
	<key>com.apple.developer.healthkit.workout-processing</key>
	<true/>

	<!-- ============================================================ -->
	<!-- PUSH NOTIFICATIONS -->
	<!-- ============================================================ -->

	<!-- Apple Push Notification Service -->
	<!-- PRODUCTION: Required for App Store / TestFlight distribution -->
	<key>aps-environment</key>
	<string>production</string>

	<!-- ============================================================ -->
	<!-- APP GROUPS -->
	<!-- ============================================================ -->

	<!-- App Groups -->
	<key>com.apple.security.application-groups</key>
	<array>
		<string>group.com.echoelmusic.shared</string>
	</array>

	<!-- ============================================================ -->
	<!-- KEYCHAIN SHARING -->
	<!-- ============================================================ -->

	<!-- Keychain Sharing -->
	<key>keychain-access-groups</key>
	<array>
		<string>$(AppIdentifierPrefix)com.echoelmusic.app</string>
	</array>

	<!-- ============================================================ -->
	<!-- SIRI & SHORTCUTS -->
	<!-- ============================================================ -->

	<!-- Siri (for watchOS Shortcuts) -->
	<key>com.apple.developer.siri</key>
	<true/>
</dict>
</plist>
