<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- HealthKit Access (basic - no health-records which requires approval) -->
	<key>com.apple.developer.healthkit</key>
	<true/>

	<!-- App Groups -->
	<key>com.apple.security.application-groups</key>
	<array>
		<string>group.com.echoelmusic.shared</string>
	</array>

	<!-- Keychain Sharing -->
	<key>keychain-access-groups</key>
	<array>
		<string>$(AppIdentifierPrefix)com.echoelmusic.app</string>
	</array>

	<!-- Background Modes -->
	<key>com.apple.developer.healthkit.background-delivery</key>
	<true/>

	<!-- Workout Processing -->
	<key>com.apple.developer.healthkit.workout-processing</key>
	<true/>

	<!-- Push Notifications - Removed: Not implemented, would cause App Store rejection -->
</dict>
</plist>
