<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>en</string>
	<key>CFBundleDisplayName</key>
	<string>Echoelmusic</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Echoelmusic</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(MARKETING_VERSION)</string>
	<key>CFBundleVersion</key>
	<string>$(CURRENT_PROJECT_VERSION)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UIApplicationSceneManifest</key>
	<dict>
		<key>UIApplicationSupportsMultipleScenes</key>
		<false/>
	</dict>
	<key>UILaunchScreen</key>
	<dict/>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UIUserInterfaceStyle</key>
	<string>Dark</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>audio</string>
		<string>bluetooth-central</string>
		<string>bluetooth-peripheral</string>
		<string>remote-notification</string>
	</array>
	<key>NSMicrophoneUsageDescription</key>
	<string>Echoelmusic uses your microphone to capture audio for bio-reactive processing and voice-to-music transformation.</string>
	<key>NSHealthShareUsageDescription</key>
	<string>Echoelmusic uses heart rate and HRV data to create personalized bio-reactive audio experiences that respond to your body.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>Echoelmusic may save your meditation and coherence training sessions to HealthKit.</string>
	<key>NSCameraUsageDescription</key>
	<string>Echoelmusic uses your camera for face tracking to control audio parameters with your facial expressions.</string>
	<key>NSFaceIDUsageDescription</key>
	<string>Echoelmusic uses TrueDepth camera for face tracking to map your expressions to audio-visual parameters.</string>
	<key>NSBluetoothAlwaysUsageDescription</key>
	<string>Echoelmusic connects to Bluetooth MIDI controllers and audio devices for professional music creation.</string>
	<key>NSBluetoothPeripheralUsageDescription</key>
	<string>Echoelmusic connects to Bluetooth MIDI controllers for music creation.</string>
	<key>NSLocalNetworkUsageDescription</key>
	<string>Echoelmusic uses local network for DMX lighting control, Art-Net, and connecting to external audio interfaces.</string>
	<key>NSBonjourServices</key>
	<array>
		<string>_http._tcp</string>
		<string>_rtsp._tcp</string>
		<string>_artnet._udp</string>
		<string>_osc._udp</string>
		<string>_midi._udp</string>
		<string>_echoelmusic._tcp</string>
	</array>
	<key>NSMotionUsageDescription</key>
	<string>Echoelmusic uses motion data for gesture-based audio control and spatial audio head tracking.</string>
	<key>NSEyeTrackingUsageDescription</key>
	<string>Echoelmusic uses eye tracking for immersive gaze-based audio control on supported devices.</string>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	<key>UIRequiresFullScreen</key>
	<false/>
	<key>UIStatusBarHidden</key>
	<true/>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<false/>
	<key>CFBundleIcons</key>
	<dict>
		<key>CFBundlePrimaryIcon</key>
		<dict>
			<key>CFBundleIconFiles</key>
			<array>
				<string>AppIcons/AppIcon60x60</string>
				<string>AppIcons/AppIcon40x40</string>
				<string>AppIcons/AppIcon29x29</string>
				<string>AppIcons/AppIcon20x20</string>
			</array>
			<key>CFBundleIconName</key>
			<string>AppIcon</string>
		</dict>
	</dict>
	<key>CFBundleIcons~ipad</key>
	<dict>
		<key>CFBundlePrimaryIcon</key>
		<dict>
			<key>CFBundleIconFiles</key>
			<array>
				<string>AppIcons/AppIcon60x60</string>
				<string>AppIcons/AppIcon76x76</string>
				<string>AppIcons/AppIcon83.5x83.5</string>
				<string>AppIcons/AppIcon40x40</string>
				<string>AppIcons/AppIcon29x29</string>
				<string>AppIcons/AppIcon20x20</string>
			</array>
			<key>CFBundleIconName</key>
			<string>AppIcon</string>
		</dict>
	</dict>
	<key>MinimumOSVersion</key>
	<string>15.0</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>arm64</string>
		<string>microphone</string>
	</array>
	<key>NSAppTransportSecurity</key>
	<dict>
		<!-- Required for local network devices: MIDI controllers, DMX fixtures, Art-Net -->
		<key>NSAllowsLocalNetworking</key>
		<true/>
		<!-- Localhost HTTP exception removed for production security -->
	</dict>
</dict>
</plist>
