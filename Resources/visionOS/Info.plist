<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>en</string>
	<key>CFBundleDisplayName</key>
	<string>Echoelmusic</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Echoelmusic</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(MARKETING_VERSION)</string>
	<key>CFBundleVersion</key>
	<string>$(CURRENT_PROJECT_VERSION)</string>
	<key>UIApplicationSceneManifest</key>
	<dict>
		<key>UIApplicationPreferredDefaultSceneSessionRole</key>
		<string>UIWindowSceneSessionRoleVolumetricApplication</string>
		<key>UIApplicationSupportsMultipleScenes</key>
		<true/>
		<key>UISceneConfigurations</key>
		<dict/>
	</dict>
	<key>UIUserInterfaceStyle</key>
	<string>Dark</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>audio</string>
	</array>
	<key>NSMicrophoneUsageDescription</key>
	<string>Echoelmusic uses your microphone for voice-controlled audio and immersive sound experiences.</string>
	<key>NSCameraUsageDescription</key>
	<string>Echoelmusic uses cameras for hand and eye tracking to control immersive audio-visual experiences.</string>
	<key>NSHandsTrackingUsageDescription</key>
	<string>Echoelmusic uses hand tracking for gesture-based control of audio parameters and immersive interactions.</string>
	<key>NSWorldSensingUsageDescription</key>
	<string>Echoelmusic uses spatial awareness to place bio-reactive audio and visual elements in your environment.</string>
	<key>NSBluetoothAlwaysUsageDescription</key>
	<string>Echoelmusic connects to Bluetooth devices for bio-data and MIDI control.</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>arm64</string>
	</array>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<!-- visionOS does not use interface orientations -->
	<key>MinimumOSVersion</key>
	<string>1.0</string>
	<key>NSHealthShareUsageDescription</key>
	<string>Echoelmusic uses heart rate and HRV data to create personalized bio-reactive audio experiences in your spatial environment.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>Echoelmusic may save your meditation and coherence training sessions to HealthKit.</string>
	<key>NSLocalNetworkUsageDescription</key>
	<string>Echoelmusic uses local network for DMX lighting control, Art-Net, and connecting to external audio interfaces.</string>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsLocalNetworking</key>
		<true/>
	</dict>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	<key>NSEyeTrackingUsageDescription</key>
	<string>Echoelmusic uses eye tracking for immersive gaze-based audio control.</string>
</dict>
</plist>
