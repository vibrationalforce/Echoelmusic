{
  "name": "Echoelmusic",
  "short_name": "Echoelmusic",
  "description": "Create from Within. Turn your heartbeat, breath, and body into music, visuals, and light. Bio-reactive creative platform.",
  "start_url": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "background_color": "#050510",
  "theme_color": "#FF1494",
  "orientation": "any",
  "scope": "/",
  "lang": "en",
  "dir": "ltr",
  "categories": ["music", "health", "productivity", "entertainment"],
  "icons": [
    {
      "src": "favicon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "app-icon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "app-icon.svg",
      "sizes": "1024x1024",
      "type": "image/svg+xml",
      "form_factor": "wide",
      "label": "Echoelmusic Bio-Reactive Interface"
    }
  ],
  "shortcuts": [
    {
      "name": "Quick Session",
      "short_name": "Session",
      "description": "Start a bio-reactive session",
      "url": "/?action=session",
      "icons": [{"src": "favicon.svg", "sizes": "any"}]
    }
  ],
  "related_applications": [
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/echoelmusic/id6757957358"
    }
  ],
  "prefer_related_applications": true,
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "share_target": {
    "action": "/",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+echoelmusic",
      "url": "/?protocol=%s"
    }
  ]
}
